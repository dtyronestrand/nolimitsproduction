{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "^18.x"
	},
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"slicemachine": "start-slicemachine"
	},
	"devDependencies": {
		"@iconify-json/ph": "^1.2.1",
		"@slicemachine/adapter-sveltekit": "^0.3.53",
		"@sveltejs/adapter-vercel": "^5.4.7",
		"@sveltejs/kit": "^2.8.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^8.56.10",
		"autoprefixer": "^10.4.20",
		"concurrently": "^8.2.2",
		"daisyui": "^4.12.14",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.3",
		"globals": "^15.4.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"slice-machine-ui": "^2.9.1",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.0",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.20",
		"typescript-svelte-plugin": "^0.3.38",
		"unplugin-icons": "^0.20.0",
		"vite": "^5.3.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/dm-sans": "^5.1.0",
		"@fontsource/protest-riot": "^5.1.0",
		"@prismicio/client": "^7.11.0",
		"@prismicio/svelte": "^1.3.0",
		"@supabase/ssr": "^0.5.2",
		"@supabase/supabase-js": "^2.46.1",
		"@sveltejs/kit": "^2.8.1",
		"@vercel/speed-insights": "^1.1.0",
		"clsx": "^2.1.1",
		"gsap": "^3.12.5"
	},
	"overrides": {
		"cookie": "^0.7.0"
	}
